      [
        "Get"
      ],
      "DownstreamPathTemplate": "/api/categories/collection/({ids})",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts":
      [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/gw/categories/collection",
      "UpstreamHttpMethod":
      [
        "Post"
      ],
      "DownstreamPathTemplate": "/api/categories/collection",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts":
      [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ]
    }
